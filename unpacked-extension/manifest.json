{
    "manifest_version": 2,
    "name": "ImageQuilts",
    "description": "ImageQuilts allows you to make \"quilts\", or matrices of images, out of Google image search results.",
    "version": "2.6",
    "permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "js/script.js",
        "js/popup.js",
        "js/findImages.js",
        "css/style.css",
        "fonts/lato-lig-webfont.ttf",
        "fonts/lato-reg-webfont.ttf"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "38": "icons/icon48.png"
        },
        "default_title": "ImageQuilts",
        "default_popup": "html/popup.html"
    }
}
