{
    "manifest_version": 2,
    "name": "Quiltify",
    "description": "Quiltify allows you to make \"quilts\", or matrices of images, out of Google image search results.",
    "version": "1.0",
    "content_scripts": [{
        "js": ["contentscript.js"],
        "matches": ["*://www.google.com/search*"]
    }],
    "web_accessible_resources": [
        "script.js"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}